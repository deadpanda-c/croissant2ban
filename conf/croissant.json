{
    "whitelist": [
        "127.0.0.1",
        "192.168.7.100",
        "10.10.10.17",
        "10.10.10.1"
    ],
    "services": {
        "sshd": {
            "name": "sshd",
            "logpath": "/var/log/auth.log",
            "port": 22,
            "enabled": true,
            "bantime": 600,
            "regex": "Connection closed by authenticating user .* ([0-9.]+) port .* \\[preauth\\]"
      },
      "httpd": {
            "name": "httpd",
            "logpath": "/var/log/nginx/error.log",
            "port": 8000,
            "enabled": true,
            "bantime": 600
      }
    }
}

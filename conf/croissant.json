{
    "services": {
        "sshd": {
            "name": "sshd",
            "logpath": "/var/log/auth.log",
            "port": 22,
            "enabled": true,
            "bantime": 600,
            "regex": "Failed password for .* from ([0-9.]+) port"
      },
      "httpd": {
            "name": "httpd",
            "logpath": "/var/log/nginx/error.log",
            "port": 8000,
            "enabled": true,
            "bantime": 600
      }
    }
}
